{
  "$schema": "https://raw.githubusercontent.com/QuestPackageManager/QPM.Package/refs/heads/main/qpm.schema.json",
  "version": "0.1.0",
  "sharedDir": "shared",
  "dependenciesDir": "extern",
  "info": {
    "name": "BeatTogether",
    "id": "BeatTogether",
    "version": "2.2.5",
    "url": "https://github.com/BeatTogether/BeatTogether.Quest",
    "additionalData": {
      "overrideSoName": "libBeatTogether.so",
      "cmake": true
    }
  },
  "workspace": {
    "scripts": {
      "build": [
        "pwsh build.ps1 $0:?"
      ],
      "clean": [
        "qpm s build -- -clean"
      ],
      "copy": [
        "pwsh copy.ps1 $0:?"
      ],
      "log": [
        "pwsh logging.ps1 $0:?"
      ],
      "qmod": [
        "pwsh buildQMOD.ps1 BeatTogether"
      ],
      "stack": [
        "pwsh ndk-stack.ps1 $0:?"
      ]
    },
    "ndk": "^27.2.12479018",
    "qmodIncludeDirs": [
      "./build",
      "./extern/libs"
    ],
    "qmodIncludeFiles": [],
    "qmodOutput": "BeatTogether.qmod"
  },
  "dependencies": [
    {
      "id": "beatsaber-hook",
      "versionRange": "^6.4.2",
      "additionalData": {}
    },
    {
      "id": "bs-cordl",
      "versionRange": "4008.*",
      "additionalData": {}
    },
    {
      "id": "multiplayer-core",
      "versionRange": "^1.6.5",
      "additionalData": {}
    },
    {
      "id": "scotland2",
      "versionRange": "^0.1.6",
      "additionalData": {
        "includeQmod": false,
        "private": true
      }
    },
    {
      "id": "paper2_scotland2",
      "versionRange": "4.6.*",
      "additionalData": {
        "private": true
      }
    },
    {
      "id": "lapiz",
      "versionRange": "^0.2.23",
      "additionalData": {}
    },
    {
      "id": "bsml",
      "versionRange": "^0.4.55",
      "additionalData": {}
    },
    {
      "id": "custom-types",
      "versionRange": "^0.18.3",
      "additionalData": {}
    },
    {
      "id": "kaleb",
      "versionRange": "^0.1.9",
      "additionalData": {}
    }
  ]
}